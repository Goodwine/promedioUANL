{
    "manifest_version": 2,
    "name": "Promedio UANL",
    "short_name": "Promedio UANL",
    "description": "Calcula tu \"Promedio General\" (Calificaciones aprobadas), y tu \"Promedio Normal\" (incluye materias reprobadas).",
    "icons": {
        "16": "img/16.png",
        "48": "img/48.png",
        "96": "img/96.png",
        "128": "img/128.png"
    },
    "content_scripts": [{
        "js": ["zepto.min.js", "content_script.js"],
        "matches": ["http://deimos.dgi.uanl.mx/cgi-bin/wspd_cgi.sh/econskdx*"],
        "all_frames": true
    }],
    "permissions": ["http://deimos.dgi.uanl.mx/*"],
    "version": "0.8"
}
